package lineAnimate
import processing.core._

object Test {
  def main(args: Array[String]) {
    PApplet.main(Array[String]("Test"))
  }
}

class Test extends PApplet {
  override def settings () {
    size(500, 500)
  }

  override def setup() {
    stroke(255)
    background(0)
    frameRate(300)
  }
}